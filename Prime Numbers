#include <stdio.h>

#include <stdlib.h>

int main() {
  int n;
  int x;
  int i;
  printf(“the prime numbers are: \n”);
  for (n = 2; n > 0; n++) {
    for (x = 2; x <= n; x++)
      if (n % x == 0)
        i++;
    if (i == 1)
      printf("%d, ", n);
    i = 0;
  }
  system("pause");
  return 0;
}
